{
    "name": "yassinedabbous/laravel-dynamic-query",
    "description": "Build query dynamicaly with your API",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/YassineDabbous/laravel-dynamic-query",
    "authors": [
        {
            "name": "Yassine Dabbous",
            "email": "yassine.dabbous@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": ["laravel", "dynamic fields", "dynamic filter", "dynamic sort", "selective columns"],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
      "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YassineDabbous\\DynamicQuery\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "YassineDabbous\\DynamicQuery\\Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YassineDabbous\\DynamicQuery\\DynamicFieldsServiceProvider"
            ]
        }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    }
}
